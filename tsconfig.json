{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "types": ["cypress", "node"],
    "baseUrl": ".",
    "paths": {
      "@utils/*": ["cypress/support/utils/*"],
      "@support/*": ["cypress/support/*"],
      "@fixtures/*": ["cypress/fixtures/*"]
    }
  },
  "include": ["cypress", "cypress.config.ts"]
}
